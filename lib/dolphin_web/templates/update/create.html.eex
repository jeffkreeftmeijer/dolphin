<h1>Update posted succesfully</h1>

<ul>
  <%= for {name, values} <- @links do %>
    <li><%= name %>
      <ul>
        <%= for url <- values do %>
          <li><%= link url, to: url %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>

<span><%= link "Back", to: Routes.update_path(@conn, :new) %></span>
